{
  "name": "vuepress-plugin-vue-example",
  "description": "A Vuepress plugin to display Vue components' live examples and source code inside documentation",
  "version": "1.1.2",
  "author": "Giannis Koutsaftakis",
  "license": "MIT",
  "repository": "https://github.com/kouts/vuepress-plugin-vue-example",
  "keywords": [
    "vuepress",
    "code",
    "example",
    "vue",
    "demo",
    "demo-code",
    "documentation"
  ],
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test:unit": "vue-cli-service test:unit",
    "watch": "vuepress dev docs",
    "lint": "eslint . --ext .js,.vue",
    "lint-fix": "eslint . --fix --ext .js,.vue"
  },
  "dependencies": {
    "raw-loader": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-unit-jest": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/test-utils": "^1.1.3",
    "bootstrap": "^4.6.0",
    "eslint": "^7.25.0",
    "eslint-config-kouts": "^1.3.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.9.0",
    "jest-raw-loader": "^1.0.1",
    "node-sass": "^4.14.1",
    "prettier": "2.2.1",
    "prismjs": "^1.23.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.8.2"
