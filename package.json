{
  "name": "vuepress-plugin-vue-example",
  "description": "A Vuepress plugin to display Vue components' live examples and source code inside documentation",
  "version": "0.0.0-semantic-release",
  "author": "Giannis Koutsaftakis",
  "license": "MIT",
  "repository": "https://github.com/kouts/vuepress-plugin-vue-example",
  "keywords": [
    "vuepress",
    "code",
    "example",
    "vue",
    "demo",
    "demo-code",
    "documentation"
  ],
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test:unit": "vue-cli-service test:unit",
    "watch": "vuepress dev docs",
    "lint": "eslint . --ext .js,.vue",
    "lint-fix": "eslint . --fix --ext .js,.vue",
    "prepare": "husky install"
  },
  "dependencies": {
    "raw-loader": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@vue/cli-plugin-babel": "^4.5.17",
    "@vue/cli-plugin-unit-jest": "^4.5.17",
    "@vue/cli-service": "^4.5.17",
    "@vue/test-utils": "^1.3.0",
    "bootstrap": "^4.6.1",
    "eslint": "^7.32.0",
    "eslint-config-kouts": "^1.3.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.20.0",
    "husky": "^7.0.4",
    "jest-raw-loader": "^1.0.1",
    "node-sass": "^4.14.1",
    "prettier": "2.6.2",
    "prismjs": "^1.28.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuepress": "^1.9.7"
  }
}
